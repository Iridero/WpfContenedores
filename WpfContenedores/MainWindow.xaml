<Window x:Class="WpfContenedores.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfContenedores"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="200">
    <StackPanel>
        <Button x:Name="btnScroll" Click="btnScroll_Click">
            ScrollViewer
        </Button>
        <Button Name="bntBordes" Click="bntBordes_Click">
            Bordes
        </Button>
        <Button x:Name="btnExpander" Click="btnExpander_Click">
            Expander
        </Button>
    </StackPanel>
</Window>
